<p id="notice"><%= notice %></p>

<h1>Listing Deploys</h1>

<table>
  <thead>
    <tr>
      <th>Componente</th>
      <th>Versao</th>
      <th>Responsavel</th>
      <th>Status</th>
      <th>Criado em</th>
      <th>Atualizado em</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @deploys.each do |deploy| %>
      <tr>
        <td><%= deploy.componente %></td>
        <td><%= deploy.versao %></td>
        <td><%= deploy.responsavel %></td>
        <td><%= deploy.status %></td>
        <td><%= deploy.created_at %></td>
        <td><%= deploy.updated_at %></td>
        <td><%= link_to 'Show', deploy %></td>
        <td><%= link_to 'Edit', edit_deploy_path(deploy) %></td>
        <td><%= link_to 'Destroy', deploy, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Deploy', new_deploy_path %>
